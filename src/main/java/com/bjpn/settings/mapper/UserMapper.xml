<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpn.settings.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bjpn.settings.bean.User">
        <id column="user_id" property="userId" />
        <result column="user_code" property="userCode" />
        <result column="user_name" property="userName" />
        <result column="user_pwd" property="userPwd" />
        <result column="user_email" property="userEmail" />
        <result column="user_expire_time" property="userExpireTime" />
        <result column="user_lock_state" property="userLockState" />
        <result column="dept_no" property="deptNo" />
        <result column="create_time" property="createTime" />
        <result column="create_by" property="createBy" />
        <result column="edit_time" property="editTime" />
        <result column="edit_by" property="editBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, user_code, user_name, user_pwd, user_email, user_expire_time, user_lock_state, dept_no, create_time, create_by, edit_time, edit_by
    </sql>

</mapper>
